@model TrackItemRowModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<tr class="track-item-row" data-item-name="@Model.Name" data-item-id="@Model.ID">
    <td>@Model.Name</td>
    <td>@Model.Title</td>
    <td class="td-subject">@Model.Subject</td>
    <td class="td-date">@Model.IssueDate.ToString("dd-MM-yyyy")</td>
    <td class="td-date">@Model.ExpiryDate.ToString("dd-MM-yyyy")</td>
    <td class="validity-@Model.Status">@Model.Status</td>
</tr>